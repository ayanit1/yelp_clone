<!DOCTYPE html>
<html>
  <head>
    <style>
       #map {
        height: 400px;
        width: 500px;
        border-style: solid;
        border-width: 1px;
       }
    </style>
  </head>
  <body>

    <div id="map"></div>
    <script>
      function initMap() {
        // replace lat/lng with restaurant.lat and restaurant.lng
        var uluru = {lat: 51.5170973, lng: -0.06598350000001574};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 16,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>

    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCKMWdQAlMLQrjXcr1uB6_Osv2w2kVgiIg&libraries=places&callback=initMap">
    </script>
  </body>
</html>
